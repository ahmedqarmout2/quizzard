.row
  .col.s12
    .card.center
      .card-image
        a.btn-floating.halfway-fab.waves-effect.waves-light.red.left#questions-import-back-button
          i.material-icons arrow_back
      .card-content
        h5.card-title Questions Import Form
        h6 Your quizzard file
      .card-action
        form(encType='multipart/form-data')#questions-import-form
          .file-field.input-field
            .btn
              span File
              input(type='file', accept='.quizzard')#questions-import-form-input
            .file-path-wrapper
              input.file-path.validate(placeholder='Select your quizzard file')
          br
          a.waves-effect.waves-light.btn(onClick='submitQuestionsImportForm()')
            i.material-icons.left cloud_upload
            | Import