.row
  .col.s12
    .card.center
      .card-image
        a.btn-floating.halfway-fab.waves-effect.waves-light.red.left#questions-export-back-button
          i.material-icons arrow_back
      .card-content
        h5.card-title Questions Export Form
        h6 Export the list of available questions
      .card-action
        #questionsExportForm
          table.bordered.highlight
              thead
                tr
                  th Included
                  th Title
                  th Topic
                  th Type
              tbody
                each q in questionsList
                  tr
                    td
                      p
                        input.checked(id='cb-' + q._id, type='checkbox', checked=true)
                        label.visbox(for='cb-' + q._id)
                    td #{q.title}
                    td #{q.topic}
                    td #{getQuestionType(q.type)}
          br
          a.waves-effect.waves-light.btn(onClick='submitQuestionsExportForm()')
            i.material-icons.left cloud_download
            | Export