ul.collection
  each q in questions
    li.collection-item.avatar.disable-select.question-list-hover(onClick='goToQuestion(this.id)', id=q._id)
      i.material-icons.circle #{getQuestionIcon(q.type)}
      span.title #{q.title}
      p
        | #{q.topic}
        br
        | #{q.points}
      a.secondary-content.question-block.noLineLink
        i.material-icons launch
        p.text-center.title